# man systemd.unit
# man systemd.service

[Unit]
Description=Consul Agent
After=network.target

[Service]
User=consul
Group=consul
ExecStart=/usr/sbin/consul agent -pid-file /var/run/consul.pid -config-dir /etc/consul/consul.d >> /var/log/consul-agent.log 
PIDFile=/var/run/consul.pid

[Install]
WantedBy=multi-user.target